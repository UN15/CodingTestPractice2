import java.util.Scanner;
import java.io.FileInputStream;

class Solution
{
	public static void main(String args[]) throws Exception
	{
		Scanner sc = new Scanner(System.in);
		int T=sc.nextInt();
        int sum = 0;
		for(int i = 1; i <= T; i++)
		{
            for(int j = 0; j<10; j++){
				int a = sc.nextInt();
            	if(a%2 !=0)
                	sum+=a;
            }
            System.out.println("#"+i+" "+sum);
            sum = 0;
		}
	}
}
