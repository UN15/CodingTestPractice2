LV 2.
import java.io.*;
class Solution
{
	public static void main(String args[]) throws Exception
	{
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        int T = Integer.parseInt(br.readLine());
        int[] a = {2, 3, 5, 7, 11};
        int[] a2 = {0, 0, 0, 0, 0};
		for(int i = 1; i <= T; i++)
		{
			int n = Integer.parseInt(br.readLine());
            int j = 0;
			while(n>1){
                if(n%a[j] == 0){
                    n/=a[j];
                    a2[j]++;
                }
                else
                    j++;
            }
            System.out.print("#"+i+" ");
			for(int k = 0; k<5; k++){
                System.out.print(a2[k]+" ");
                a2[k] = 0;
            }
            System.out.println();
		}
	}
}

import java.io.*;
 
class Solution
{
	public static void main(String args[]) throws Exception
	{
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int T = Integer.parseInt(br.readLine());
		for(int i = 1; i <= T; i++)
		{
			int N = Integer.parseInt(br.readLine());
            int sum = 0;
            for(int j = 1; j<=N; j++){
                if(j%2 != 0)
                    sum+=j;
                else
                    sum-=j;
            }
			System.out.println("#"+i+" "+sum);
		}
	}
}

import java.io.*;
class Solution
{
    public static void main(String args[]) throws Exception
    {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        int T = Integer.parseInt(br.readLine());
     
        for(int test_case = 1; test_case <= T; test_case++)
        {
            String N = br.readLine();
            String answer = count(N);
            System.out.println("#"+test_case+" "+answer);
        }
    }
    public static String count(String N){
        int[] arr = new int[10];
        int c = 0;
        String n = " ";
        boolean check = true;
        while(true){
            check = true;
            for(int i = 0; i<10; i++){
                if(arr[i] == 0){
                    check = false;
                    break;
                }
            }
             
            if(check)
                return n;
            c++;
            n = String.valueOf(Integer.parseInt(N)*c);
             
            for(int j = 0; j<n.length(); j++){
                if(arr[Integer.parseInt(n.substring(j, j+1))] == 0)
                    arr[Integer.parseInt(n.substring(j, j+1))] = 1;
            }
        }
    }
}

import java.io.*;
import java.util.StringTokenizer;
class Solution{
    public static void main(String args[]) throws Exception
        {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        int T = Integer.parseInt(br.readLine());
        StringTokenizer st;
        int A, B;
        for(int i = 1; i <= T; i++){
            st = new StringTokenizer(br.readLine());
            int P = Integer.parseInt(st.nextToken());
            int Q = Integer.parseInt(st.nextToken());
            int R = Integer.parseInt(st.nextToken());
            int S = Integer.parseInt(st.nextToken());
            int W = Integer.parseInt(st.nextToken());
            A = P*W;
            if(W<=R)
                B = Q;
            else
                B = Q+(W-R)*S;
            if(A<B)
                System.out.println("#"+i+" "+A);
            else
                System.out.println("#"+i+" "+B);
        }
    }
}

import java.io.*;
class Solution
{
    public static void main(String args[]) throws Exception
    {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        int T= Integer.parseInt(br.readLine());
     
        for(int i = 1; i<= T; i++)
        {
            char[] carr = br.readLine().toCharArray();
            boolean check = true;
            for(int j = 0; j<carr.length/2; j++){
                if(carr[j] != carr[carr.length-j-1]){
                    check= false;
                    break;
                }
            }
            if(check)
                System.out.println("#"+i+" "+1);
            else
                System.out.println("#"+i+" "+0);
 
        }
    }
}

import java.io.*;
import java.util.StringTokenizer;
class Solution
{
	public static void main(String args[]) throws Exception
	{
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int T=Integer.parseInt(br.readLine());
		StringTokenizer st;
		for(int i = 1; i <= T; i++)
		{
			st = new StringTokenizer(br.readLine());
            int N = Integer.parseInt(st.nextToken());
            int M = Integer.parseInt(st.nextToken());
           
            String[] a1 = br.readLine().split(" ");
            String[] a2 = br.readLine().split(" ");
            int max = Math.max(N, M);
            int min = Math.min(N, M);
            if(a1.length == max)
                System.out.println("#"+i+" "+calc(a1, a2));
            else
                System.out.println("#"+i+" "+calc(a2, a1));
		}
	}
    public static int calc(String[] max, String[] min){
        int m = 0;
        for(int i = 0; i<=max.length-min.length; i++){
            int sum = 0;
            for(int j = 0; j<min.length; j++){
                sum+= Integer.parseInt(min[j])*Integer.parseInt(max[i+j]);
            }
            m = Math.max(sum, m);
        }
        return m;
    }
}

import java.io.*;
class Solution
{
	public static void main(String args[]) throws Exception
	{
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int T=Integer.parseInt(br.readLine());
		String repeat = "";
        String check = "";
		for(int i = 1; i <= T; i++){
			String s = br.readLine();
            int len = 1;
			for(int j = 0; j<10; j++){
                repeat = s.substring(j, j+len);
                check = s.substring(j+len, j+2*len);
                if(repeat.equals(check)){
                    System.out.println("#"+i+" " + len);
                    break;
                }
                len++;
            }
		}
	}
}

import java.io.*;
import java.util.StringTokenizer;
import java.util.Arrays;
class Solution
{
	public static void main(String args[]) throws Exception{
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int T=Integer.parseInt(br.readLine());
		StringTokenizer st;
		for(int i = 1; i <= T; i++){
			int N = Integer.parseInt(br.readLine());
			int[] arr = new int[N];
            st = new StringTokenizer(br.readLine());
            for(int j = 0; j<N; j++)
                arr[j] = Integer.parseInt(st.nextToken());
            
            Arrays.sort(arr);
            System.out.print("#"+i+" ");
            for(int k : arr)
                System.out.print(k+" ");
            System.out.println();
		}
	}
}
