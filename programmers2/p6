import java.util.Arrays;
class Solution {
    public String solution(String s) {
        String answer = "";
        String[] arr = s.split(" ");
        int[] a = new int[arr.length];
        for(int i = 0; i<arr.length; i++)
            a[i] = Integer.parseInt(arr[i]);
        
        Arrays.sort(a);
        answer+=a[0]+" "+a[a.length-1];
       
        return answer;
    }
}
